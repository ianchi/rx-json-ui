{
  "name": "reactive-json-form-ng",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist-lib-init": "rm rf dist/reactive-json-form-ng && git worktree add dist/reactive-json-form-ng deploy",
    "build-lib": "rm -rf dist/reactive-json-form-ng/* && ng build reactive-json-form-ng --prod && npm run bundle-styles",
    "watch-lib": "ng build reactive-json-form-ng --watch",
    "deploy-lib": "cd dist/reactive-json-form-ng && git add --all && git commit -m 'release' && git push",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged --pattern \"*.{ts,scss,css,html}\""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "core-js": "^3.1.4",
    "espression": "^1.2.0",
    "espression-rx": "^0.3.1",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "^0.801.1",
    "@angular/cli": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "^8.1.1",
    "codelyzer": "^5.0.1",
    "husky": "^3.0.0",
    "ng-packagr": "^5.3.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "scss-bundle": "^2.5.1",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "~3.4.5"
  }
}
