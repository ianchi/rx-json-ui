{
  "name": "reactive-json-form-ng",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist-lib-init": "rm rf dist/reactive-json-form-ng && git worktree add dist/reactive-json-form-ng deploy",
    "build-lib": "rm -rf dist/reactive-json-form-ng/* && ng build reactive-json-form-ng --prod && npm run bundle-styles",
    "watch-lib": "ng build reactive-json-form-ng --watch",
    "deploy-lib": "cd dist/reactive-json-form-ng && git add --all && git commit -m 'release' && git push",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "precise-commits": "precise-commits"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/router": "^7.2.8",
    "core-js": "^2.6.5",
    "espression": "^1.1.1",
    "espression-rx": "^0.2",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.5",
    "@angular-devkit/build-ng-packagr": "~0.13.5",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "codelyzer": "^4.5.0",
    "husky": "^1.3.1",
    "ng-packagr": "^4.2.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.4",
    "scss-bundle": "^2.5.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "^5.13.1",
    "typescript": "~3.2.2"
  }
}
